@{
    Layout = null;
}

<!DOCTYPE html>

<html style="overflow: hidden">
<head>
    <meta name="viewport" content="width=device-width"/>
    <style>
        .unselectable {
            -moz-user-select: -moz-none;
            -khtml-user-select: none;
            -webkit-user-select: none;
            -o-user-select: none;
            user-select: none;
        }

        body {
            background: white;
        }

        .popup {
            position: absolute;
            z-index: 2;
        }

        #wrapper {
            position: fixed;
            top: 0;
            left: 0;
            display: block;
            width: 100%;
            height: 100%;
            border: 0 none;
            z-index: 9;
            overflow: hidden;
        }

        #html_frame {
            position: absolute;
            top: 0;
            left: 0;
            border: 0px;
            zoom: 1;
            -moz-transform: translate(0px,0px);
            -moz-transform-origin: 0 0;
            -o-transform: translate(0px,0px);
            -o-transform-origin: 0 0;
            -webkit-transform: translate(0px,0px);
            -webkit-transform-origin: 0 0;
        }

        #element::-webkit-scrollbar {
            display: none;
        }

        ::-webkit-scrollbar {
            display: none;
        }

        body {
            overflow: hidden;
            margin: 0px;
            color: #FFF;
        }
    </style>






    <title></title>
</head>
<body unselectable="on" class="unselectable">
    <script src="../../Scripts/jquery-2.1.4.min.js"></script>
    <script src="../../Scripts/jquery.signalR-2.2.0.min.js"></script>
    <script src="../../Scripts/jquery.csv-0.71.min.js"></script>
<script>
    $(function() {
        $(document)
            .ready(function() {
                var gdo = parent.gdo;
                gdo.net.app["ResponsiveHTML"].initClient();
            });
    });
</script>
    <div id="wrapper">
        <iframe id="html_frame" frameborder="0" style="border: 0 none; display: block; overflow: hidden;"></iframe>
    </div>
